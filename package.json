{
  "name": "node-boilerplate",
  "version": "1.0.0",
  "private": false,
  "scripts": {
    "prestart": "npm-run-all --serial lint",
    "start": "nodemon -r dotenv/config --exec babel-node app.js",
    "lint": "eslint src/ app.js --max-warnings 0",
    "clean": "rimraf dist",
    "build-dev": "cross-env-shell BUILD_ENV=dev npm run build",
    "build-uat": "cross-env-shell BUILD_ENV=uat npm run build",
    "build-test": "cross-env-shell BUILD_ENV=test npm run build",
    "build": "npm-run-all --serial clean lint webpack",
    "webpack": "webpack --config ./.scripts/webpack.config.js"
  },
  "dependencies": {
    "async": "^3.2.4",
    "babel-polyfill": "^6.26.0",
    "babel-register": "^6.26.0",
    "bcrypt": "^5.1.0",
    "body-parser": "^1.20.2",
    "cluster": "^0.7.7",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "http-errors": "^2.0.0",
    "ioredis": "^5.3.1",
    "jsonwebtoken": "^9.0.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "mysql2": "^3.2.0",
    "nocache": "^3.0.4",
    "sequelize": "^6.29.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.21.0",
    "@babel/eslint-parser": "^7.19.1",
    "@babel/node": "^7.20.7",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-optional-chaining": "^7.21.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.21.0",
    "@babel/preset-env": "^7.20.2",
    "@babel/runtime": "^7.21.0",
    "babel-loader": "^9.1.2",
    "copy-webpack-plugin": "^11.0.0",
    "core-js": "^3.29.0",
    "cross-env": "^7.0.3",
    "dotenv-webpack": "^8.0.1",
    "eslint": "^8.35.0",
    "eslint-plugin-lodash": "^7.4.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "eslint-webpack-plugin": "^4.0.0",
    "nodemon": "^2.0.21",
    "npm-run-all": "^4.1.5",
    "rimraf": "^4.3.0",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-middleware": "^6.0.1",
    "webpack-node-externals": "^3.0.0"
  }
}
